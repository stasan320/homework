#include <iostream>

int main() {
	int d = 0, i = 1, j = 0;
	int name1, name2;

	std::cin >> name1 >> name2;

	while (i != 0) {
		if ((name1 % 10) != (name2 % 10)) {
			d++;
		}
		name1 = (name1 - (name1 % 10)) / 10;
		name2 = (name2 - (name2 % 10)) / 10;
		if ((name1 == 0) && (name2 == 0)) {
			i = 0;
			std::cout << d;
		}
		if (((name1 == 0) && (name2 != 0)) | ((name1 != 0) && (name2 == 0))) {
			i = 0;
			std::cout << "Error";
		}
	}
}
