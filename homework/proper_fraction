#include <iostream>

using namespace std;

void proper_fraction() {
	int i, j, a = 0, b, k;

	cin >> b;
	int* Array = new int[b]; 
	k = b - 1;

	for (i = 2; i < (b / 2); i++) {
		if ((b % i) == 0) {
			Array[a] = i;
			a++;
		}
	}

	for (i = 1; i < b; i++) {
		for (j = 0; j < a; j++) {
