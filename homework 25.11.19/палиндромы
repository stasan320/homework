#include <iostream>
#include <math.h>

using namespace std;

int main(){
    int n, i, k = 0;
    cin >> n;

    if(n % 2 == 0) {
        int *l = new int[n / 2];
        l[0] = 1;
        for(i = 1; i < (n / 2); i++){
            l[i] = 0;
        }

        while(k != (pow(10, (n / 2)) - 1)) {
            for (i = 0; i < (n / 2 + 1); i++) {
                if (l[n / 2 + 1 - i] == 10) {
                    l[n / 2 + 1 - i] = 0;
                    l[n / 2 - i]++;
                }
            }

            for (i = 0; i < (n / 2); i++) {
                cout << l[i];
            }
            for (i = (n / 2 - 1); i >= 0; i--) {
                cout << l[i];
            }
            cout << endl;
            l[n / 2 - 1] = l[n / 2 - 1] + 1;
            k++;
        }
    }
    else {
        int *l = new int[(n / 2) + 1];
        l[0] = 1;
        for(i = 1; i < (n / 2 + 1); i++){
            l[i] = 0;
        }

        while(k != (pow(10, (n / 2 + 1)) - 1)) {
            for (i = 0; i < (n / 2 + 1); i++) {
                if (l[n / 2 + 1 - i] == 10) {
                    l[n / 2 + 1 - i] = 0;
                    l[n / 2 - i]++;
                }
            }

            for (i = 0; i < (n / 2 + 1); i++) {
                cout << l[i];
            }
            for (i = (n / 2 - 1); i >= 0; i--) {
                cout << l[i];
            }
            cout << endl;
            l[n / 2] = l[n / 2] + 1;
            k++;
        }
    }
}
